<h1>New Article</h1>
<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :Author %>
    <%= f.text_field :author %>
  </p>

  <p> 
    <%= f.label :Title %>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :Body %>
    <%= f.text_area :body %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', articles_path %>
        